{
  "openapi": "3.1.0",
  "info": {
    "title": "Receipt Scanner API",
    "version": "1.0.0"
  },
  "servers": [
    {
      "url": "https://script.google.com/macros/s/AKfycbwCJMWjm3uhxSkAphvWPxNVdVj6RjnIjgufEjEeZ_7A8ZqwGGq1PYNWCJcrokHHJW08"
    }
  ],
  "paths": {
    "/exec": {
      "post": {
        "operationId": "addReceipt",
        "summary": "Add receipt data",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "store": {"type": "string"},
                  "total": {"type": "number"},
                  "date": {"type": "string"},
                  "category": {"type": "string"},
                  "tax": {"type": "number"},
                  "paymentMethod": {"type": "string"},
                  "items": {
                    "type": "array",
                    "items": {
                      "type": "object",
                      "properties": {
                        "name": {"type": "string"},
                        "price": {"type": "number"},
                        "quantity": {"type": "number"}
                      }
                    }
                  },
                  "notes": {"type": "string"}
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Success"
          }
        }
      }
    }
  }
}
